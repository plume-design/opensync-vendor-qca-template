config interface 'loopback'
	option ifname		lo
	option proto		static
	option ipaddr		127.0.0.1
	option netmask		255.0.0.0

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '0t 1 2 3 4'

config switch_vlan
	option device 'switch0'
	option vlan '2'
	option ports '0t 5'

config switch_vlan
	option device 'switch0'
	option vlan '4'
	option ports '0t 5t'
